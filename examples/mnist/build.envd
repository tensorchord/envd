def build():
    base(os="ubuntu20.04", language="python3")
    install.vscode_extensions([
        "ms-python.python",
    ])

    install.python_packages([
        "tensorflow",
        "numpy",
    ])
    install.cuda(version="11.6", cudnn="8")
    config.gpu(count=-1)
    shell("zsh")
    config.jupyter(password="", port=8888)
