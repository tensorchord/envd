def build():
    base(os="ubuntu20.04", language="python3.8")
    install.conda_packages(name = [
        "pytorch",
    ],envFile="env.yaml")

    install.python_packages(name = [
        "flask"
    ])
    install.system_packages(name = [
        "libgl1-mesa-glx",
        "openmim"
    ])
    run(commands=[
    "mim install mmcv-full",
    "mim install mmdet==2.24.0",
    ])
